<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="421.0" prefWidth="512.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ma.fstt.trackingl.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label text="Nom :" />
      <TextField fx:id="nom" />
      <Label text="Telephone" />
      <TextField fx:id="tele" />
       <Button  mnemonicParsing="false" onAction="#onUpdateButtonClick" text="edite" />
       <Button  mnemonicParsing="false" onAction="#removeLivreur" text="remove" />
        <Button fx:id="ok" mnemonicParsing="false" onAction="#onSaveButtonClick" text="Save" />
      <TableView fx:id="mytable" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="col_id" prefWidth="75.0" text="id" />
          <TableColumn fx:id="col_nom" prefWidth="75.0" text="nom" />
            <TableColumn fx:id="col_tele" prefWidth="75.0" text="telephone" />
        </columns>
      </TableView>
   </children>

 
</VBox>
